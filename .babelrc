{
  "presets": [
    "env",
    "@babel/typescript",
    "es2015"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-async-to-generator",
    "@babel/plugin-transform-typescript",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
